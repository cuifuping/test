<?xml version="1.0" encoding="UTF-8"?>
<excelTemps>
	<excelTemp>
		<name>配置文件名称（唯一）</name>
		<!--
			sheet：excel中的工作簿
			name：sheet的名称
			target-bean：sheet对应bean的名称
			date-format：日期格式
			date-delimiter：日期分隔符
			time-delimiter：时间分隔符
			four-digit-years：年份位数是否4位
			decimal-symbol：小数分隔符
			thousand-separntor：千位分隔符
			mode：导入模式
		-->
		<sheetTemps>
			<sheetTemp>
				<name>sheet名称</name>
				<target-bean>类全限定名</target-bean>
				<date-format>YMD</date-format>
				<date-delimiter>/</date-delimiter>
				<time-delimiter>:</time-delimiter>
				<four-digit-years>true/false</four-digit-years>
				<decimal-symbol>.</decimal-symbol>
				<thousand-separator>,</thousand-separator>
				<mode></mode>
				<!--
					field-name-row：字段行
				-->
				<field-name-row>0</field-name-row>

				<!-- 
					data-rows：数据行
				-->
				<data-rows>
					<data-row>
						<start-row>1</start-row>
						<end-row>4</end-row>.
					</data-row>
					<data-row>
						<start-row>7</start-row>
						<end-row>9</end-row>.
					</data-row>
				</data-rows>
				<!-- 
					field-mappings：excel字段与bean字段对应 
					target：bean字段
					source：excel字段
				-->
				<field-mappings>
					<field-mapping>
						<source-field>名称</source-field>
						<target-field>name</target-field>
					</field-mapping>
					<field-mapping>
						<source-field>密码</source-field>
						<target-field>password</target-field>
					</field-mapping>
					<field-mapping>
						<source-field>名称</source-field>
						<target-field>name</target-field>
					</field-mapping>
					<field-mapping>
						<source-field>密码</source-field>
						<target-field>password</target-field>
					</field-mapping>
				</field-mappings>
			</sheetTemp>
			<sheetTemp>
				<name>sheet名称</name>
				<target-bean>类全限定名</target-bean>
				<date-format>YMD</date-format>
				<date-delimiter>/</date-delimiter>
				<time-delimiter>:</time-delimiter>
				<four-digit-years>true/false</four-digit-years>
				<decimal-symbol>.</decimal-symbol>
				<thousand-separntor>,</thousand-separntor>
				<mode></mode>
				<!--
					field-name-row：字段行
				-->
				<field-name-row>0</field-name-row>

				<!-- 
					data-rows：数据行
				-->
				<data-rows>
					<data-row>
						<start-row>1</start-row>
						<end-row>4</end-row>.
					</data-row>
					<data-row>
						<start-row>7</start-row>
						<end-row>9</end-row>.
					</data-row>
				</data-rows>
				<!-- 
					field-mappings：excel字段与bean字段对应 
					target：bean字段
					source：excel字段
				-->
				<field-mappings>
					<field-mapping>
						<source-field>名称</source-field>
						<target-field>name</target-field>
					</field-mapping>
					<field-mapping>
						<source-field>密码</source-field>
						<target-field>password</target-field>
					</field-mapping>
					<field-mapping>
						<source-field>名称</source-field>
						<target-field>name</target-field>
					</field-mapping>
					<field-mapping>
						<source-field>密码</source-field>
						<target-field>password</target-field>
					</field-mapping>
				</field-mappings>
			</sheetTemp>
		</sheetTemps>
	</excelTemp>
</excels>